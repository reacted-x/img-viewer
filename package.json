      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@beisen-phoenix/icon": "^1.0.20",
    "@beisen-phoenix/style-token": "^1.0.38",
    "@beisen-phoenix/tooltip": "^1.0.33"
